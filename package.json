    "react-bootstrap": "^2.10.4",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-scroll": "^1.9.0",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "babel-loader": "^9.1.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^7.1.2",
