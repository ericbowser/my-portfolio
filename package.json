{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server",
    "server": "build && tail && ngr",
    "webpack:prod": "webpack-dev-server --mode=production",
    "webpack:dev": "webpack-dev-server --mode=development",
    "build": "webpack build --mode=production && npm run tail",
    "start": "webpack-dev-server",
    "tail": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "env": "npx dotenv-vault@latest pull",
    "clean": "rm node_modules -R && npm install"
  },
